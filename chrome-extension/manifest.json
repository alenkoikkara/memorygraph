{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Memento Saver",
  "description": "Save URLs to Memento",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png"
  }
} 